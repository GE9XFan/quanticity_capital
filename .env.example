# Copy to .env and adjust values for local development.
ENVIRONMENT=development
REDIS_URL=redis://localhost:6379/0

# Required for Unusual Whales REST ingestion
UNUSUAL_WHALES_API_TOKEN=

# Optional REST ingestion settings (defaults shown)
# TARGET_SYMBOLS=SPY,QQQ,IWM
# STORE_TO_REDIS=true
# ENABLE_HISTORY_STREAMS=true
# FETCH_INTERVAL_SECONDS=0
# REDIS_STREAM_MAXLEN=5000
# STORE_TO_POSTGRES=false
# POSTGRES_DSN=postgresql://postgres:postgres@localhost:5432/quanticity
# REQUEST_TIMEOUT_SECONDS=30.0
# RATE_LIMIT_REQUESTS_PER_MINUTE=100
# RATE_LIMIT_LEEWAY_SECONDS=0.5

# WebSocket ingestion (disabled by default)
# ENABLE_WEBSOCKET=false
# UNUSUAL_WHALES_WEBSOCKET_URL=wss://api.unusualwhales.com/socket
# WEBSOCKET_RECONNECT_SECONDS=5.0
